CREATE TABLE tbl_business_cards (
    cardID int NOT NULL PRIMARY KEY AUTO_INCREMENT,
    firstName varchar(255),
    lastName varchar(255),
    profession varchar(255),
    email varchar(255),
    tel varchar(255),
    website varchar(255),
    company varchar(255),
    meta text
);



CRATE TABLE tbl_account (
    ID_account int NOT NULL PRIMARY KEY AUTO_INCREMENT,
);